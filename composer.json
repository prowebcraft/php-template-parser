{
    "name": "prowebcraft/php-template-parser",
    "description": "Simple Template Parser with nested conditions",
    "type": "library",
    "require": {
        "php": "8.0.*|8.1.*",
        "prowebcraft/dot": "dev-master"
    },
    "require-dev": {
        "phpunit/phpunit": "^9.5.20",
        "infection/infection": "^0.26.10",
        "doctrine/coding-standard": "^9.0",
        "vimeo/psalm": "^4.23.0"
    },
    "license": "MIT",
    "autoload": {
        "psr-4": {
            "prowebcraft\\template\\": "src/"
        }
    },
    "authors": [
        {
            "name": "Andrey Mistulov",
            "email": "prowebcraft@gmail.com"
        }
    ],
    "minimum-stability": "dev",
    "config": {
        "allow-plugins": {
            "infection/extension-installer": true,
            "dealerdirect/phpcodesniffer-composer-installer": true
        }
    }
}
